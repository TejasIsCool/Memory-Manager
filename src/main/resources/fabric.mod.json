{
	"schemaVersion": 1,
	"id": "memory-manager",
	"version": "${version}",
	"name": "Memory Manager",
	"description": "A Memory Observation Mod",
	"authors": [
		"TejasisAmazing"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/memory-manager/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.tejasisamazing.MemoryManager"
		],
		"client": [
			"com.tejasisamazing.MemoryManagerClient"
		]
	},
	"mixins": [
		"memory-manager.mixins.json",
		{
			"config": "memory-manager.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*",
		"owo-lib": ">=0.12.21"
	}
}